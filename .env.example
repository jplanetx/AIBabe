# Supabase Configuration
# Ensure DATABASE_URL uses port 6543 for PgBouncer (Prisma connection pooling)
DATABASE_URL="postgresql://postgres:[YOUR-PASSWORD]@[YOUR-SUPABASE-HOST]:6543/[YOUR-DB-NAME]?pgbouncer=true"
NEXT_PUBLIC_SUPABASE_URL="YOUR_SUPABASE_URL"
NEXT_PUBLIC_SUPABASE_ANON_KEY="YOUR_SUPABASE_ANON_KEY"
SUPABASE_SERVICE_ROLE_KEY="YOUR_SUPABASE_SERVICE_ROLE_KEY" # For admin tasks or specific server-side operations

# Pinecone Configuration
PINECONE_API_KEY="YOUR_PINECONE_API_KEY"
PINECONE_ENVIRONMENT="YOUR_PINECONE_ENVIRONMENT"
PINECONE_INDEX_NAME="your-pinecone-index-name"

# OpenAI Configuration
OPENAI_API_KEY="YOUR_OPENAI_API_KEY"

# General Application Configuration
NODE_ENV="development" # Or "production", "test"
